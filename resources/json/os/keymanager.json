{   
      "type": "keymanager",
      "provider": "wso2",
      "serviceGroup": "apim",
      "host": "keymanager.am.wso2.com",
      "displayName": "API Manager - Keymanager",
      "description": "API Manager - Keymanager Cartridge",
      "version": "1.6.0",     
      "multiTenant": "true",
      "portMapping": [
         {
            "protocol": "http",
            "port": "9763",
            "proxyPort": "80"
         },
         {
            "protocol": "https",
            "port": "9443",
            "proxyPort": "443"
         }
       ],
      "deployment": {
        },
       "iaasProvider": [
        {
          "type": "openstack",
          "imageId": "REGION/BASE_IMAGE_ID",          
          "property": [
            {
             "name": "instanceType",
             "value": "REGION/3"
            }
          ]
        }
      ],
      "loadBalancer": {
        "type": "lb",
        "property": {
            "name": "default.load.balancer",
            "value": "true"
        }
      },
      "property": [        
        {
          "name": "payload_parameter.SERIVCE_GROUP",
          "value": "apim"
        }       
      ]    
 }
