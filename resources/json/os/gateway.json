{   
      "type": "gateway",
      "provider": "internal",
      "serviceGroup": "apim",
      "host": "gateway.am.wso2.com",
      "displayName": "API Manager - Gateway",
      "description": "API Manager - Gateway Cartridge",
      "version": "1.6.0",
      "multiTenant": "true",
      "portMapping": [
         {
            "protocol": "http",
            "port": "9763",
            "proxyPort": "80"
         },
         {
            "protocol": "https",
            "port": "9443",
            "proxyPort": "443"
         },
	{
            "protocol": "http",
            "port": "8280",
            "proxyPort": "8280"
         },
         {
            "protocol": "https",
            "port": "8243",
            "proxyPort": "8243"
         }

       ],
      "deployment": {
        },
       "iaasProvider": [
        {
          "type": "openstack",
          "imageId": "REGION/BASE_IMAGE_ID",          
          "property": [
            {
             "name": "instanceType",
             "value": "REGION/3"
            }
          ]
        }
      ],
      "loadBalancer": {
          "type": "lb",
          "property": 
             {
               "name": "default.load.balancer",
               "value": "true"
             }
      },
      "property": [
        {
          "name": "PRIMARY",
          "value": "true"
        },
        {
          "name": "payload_parameter.SERIVCE_GROUP",
          "value": "apim"
        },
        {
           "name": "payload_parameter.DEPLOYMENT",
           "value": "worker"
        },
        {
           "name": "payload_parameter.MANAGER_SERVICE_TYPE",
           "value": "gatewaymgt"
        },
        {
           "name": "payload_parameter.WORKER_SERVICE_TYPE",
           "value": "gateway"
        },
        {
          "name": "payload_parameter.CLUSTERING",
          "value": "true"
        },
        {
          "name": "payload_parameter.COMMIT_ENABLED",
          "value": "false"
        },        
        {
          "name": "payload_parameter.KEYMANAGER_HOST_NAME",
          "value": "keymanager.am.wso2.com"
        }
      ]
 }
