{
      "type": "lb",
      "provider": "lb",
      "host": "lb.ppaas.wso2.com",
      "displayName": "load balancer",
      "description": "LB Cartridge",
      "version": "4",
      "defaultAutoscalingPolicy": "economyPolicy",
      "defaultDeploymentPolicy": "deployf",
      "multiTenant": "false",
      "portMapping": [
         {
      "protocol": "http",
          "port": "8280",
          "proxyPort": "8280"
         },
         {
      "protocol": "https",
          "port": "8243",
          "proxyPort": "8243"
         }
       ],
      "deployment": {
        },
      "iaasProvider": [
         {
          "type": "openstack",
          "imageId": "REGION/BASE_IMAGE_ID",          
          "property": [
            {
             "name": "instanceType",
             "value": "REGION/3"
            }
           ]
         }
      ],
      "loadBalancer": {
        },
      "property": [
        {
          "name": "load.balancer",
          "value": "true"
        }
      ]
}
