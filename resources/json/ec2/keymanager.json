{   
      "type": "keymanager",
      "provider": "application",
      "serviceGroup": "apim",
      "host": "keymanager.am.wso2.com",
      "displayName": "API Manager - Keymanager",
      "description": "API Manager - Keymanager Cartridge",
      "version": "1.7.0",
      "multiTenant": "true",
      "portMapping": [
         {
            "protocol": "http",
            "port": "9763",
            "proxyPort": "80"
         },
         {
            "protocol": "https",
            "port": "9443",
            "proxyPort": "443"
         }
       ],
      "deployment": {
        },
      "iaasProvider": [
        {
          "type": "ec2",
          "imageId": "REGION/BASE_IMAGE_ID",
          "maxInstanceLimit": "4",
          "property": [
            {
             "name": "instanceType",
             "value": "m1.medium"
            }
          ]
        }
      ],
      "loadBalancer": {
        "type": "lb",
        "property": {
            "name": "default.load.balancer",
            "value": "true"
        }
      },
      "property": [        
        {
          "name": "payload_parameter.SERIVCE_GROUP",
          "value": "apim"
        },
        {
          "name": "PRIMARY",
          "value": "@PRIMARY"
        },
        {
          "name": "payload_parameter.CLUSTERING",
          "value": "@CLUSTERING"
        }
      ]
 }
