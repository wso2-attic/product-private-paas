{
      "type": "appservermgt",
      "provider": "wso2",
      "serviceGroup": "appservergroup",      
      "host": "mgt.appserver.wso2.com",
      "displayName": "AppServer Manager",
      "description": "AppServer Manager Cartridge",
      "version": "5.2.1",
      "multiTenant": "true",
      "portMapping": [
         {
            "protocol": "http",
            "port": "9763",
            "proxyPort": "80"
         },
         {
            "protocol": "https",
            "port": "9443",
            "proxyPort": "443"
         }
       ],
      "deployment": {
        },
      "iaasProvider": [
        {
          "type": "ec2",
          "imageId": "REGION/BASE_IMAGE_ID",
          "maxInstanceLimit": "4",
          "property": [
            {
             "name": "instanceType",
             "value": "m1.medium"
            }
          ]
        }
      ],
      "loadBalancer": {
        "type": "lb",
        "property": {
            "name": "default.load.balancer",
            "value": "true"
         }
      },
      "property": [
         {
            "name": "payload_parameter.SERIVCE_GROUP",
            "value": "appservergroup"
         },
         {
            "name": "payload_parameter.DEPLOYMENT",
            "value": "manager"
         },
         {
            "name": "payload_parameter.MANAGER_SERVICE_TYPE",
            "value": "appservermgt"
         },
         {
            "name": "payload_parameter.WORKER_SERVICE_TYPE",
            "value": "appserverworker"
         },
         {
            "name": "PRIMARY",
            "value": "true"
         },
         {
            "name": "payload_parameter.CLUSTERING",
            "value": "true"
         },
         {
            "name": "payload_parameter.COMMIT_ENABLED",
            "value": "true"
         }
      ]    
}
