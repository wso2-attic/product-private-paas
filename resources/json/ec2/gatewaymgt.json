{   
      "type": "gatewaymgt",
      "provider": "internal",
      "serviceGroup": "apim",
      "host": "mgt.gateway.am.wso2.com",
      "displayName": "API Manager - Gateway Manager",
      "description": "API Manager - Gateway Manager Cartridge",
      "version": "1.7.0",
      "multiTenant": "true",
      "portMapping": [
         {
            "protocol": "http",
            "port": "9763",
            "proxyPort": "80"
         },
         {
            "protocol": "https",
            "port": "9443",
            "proxyPort": "443"
         },
	 {
            "protocol": "http",
            "port": "8280",
            "proxyPort": "8280"
         },
         {
            "protocol": "https",
            "port": "8243",
            "proxyPort": "8243"
         }
      ],
      "deployment": {
        },
      "iaasProvider": [
        {
          "type": "ec2",
          "imageId": "REGION/BASE_IMAGE_ID",
          "maxInstanceLimit": "4",
          "property": [
            {
             "name": "instanceType",
             "value": "m1.medium"
            }
          ]
        }
      ],
      "loadBalancer": {
         "type": "lb",
         "property": {
             "name": "default.load.balancer",
              "value": "true"
        }
      }, 
      "property": [
          {
             "name": "PRIMARY",
             "value": "true"
          },
          {
             "name": "payload_parameter.SERIVCE_GROUP",
             "value": "apim"
          },
          {
             "name": "payload_parameter.DEPLOYMENT",
             "value": "manager"
          },
          {
             "name": "payload_parameter.WORKER_SERVICE_TYPE",
             "value": "gateway"
          },
          {
             "name": "payload_parameter.CLUSTERING",
             "value": "true"
          },
          {
             "name": "payload_parameter.COMMIT_ENABLED",
             "value": "true"
          },
          {
             "name": "payload_parameter.KEYMANAGER_HOST_NAME",
             "value": "keymanager.am.wso2.com"
          }
      ]    
 }
