{
      "type": "lb",
      "provider": "lb",
      "host": "isuruh.lk",
      "displayName": "load balancer",
      "description": "LB Cartridge",
      "version": "4",
      "defaultAutoscalingPolicy": "economyPolicy",
      "defaultDeploymentPolicy": "deployf",
      "multiTenant": "false",
      "portMapping": [
         {
            "protocol": "http",
            "port": "8280",
            "proxyPort": "8280"
         },
         {
            "protocol": "https",
            "port": "8243",
            "proxyPort": "8243"
         }
       ],
      "deployment": {
        },
      "iaasProvider": [
         {
            "type": "ec2",
            "imageId": "REGION/BASE_IMAGE_ID",
            "maxInstanceLimit": "5",
            "property": [
               {
                    "name": "instanceType",
                    "value": "m1.medium"
               }
            ]
         }
      ],
      "loadBalancer": {
        },
      "property": [
          {
             "name": "load.balancer",
             "value": "true"
          }
      ]
}
