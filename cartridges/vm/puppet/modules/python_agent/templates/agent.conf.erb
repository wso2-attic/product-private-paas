listen.address                        =localhost
thrift.receiver.ip                    =<%= @cep_ip %>
thrift.receiver.port                  =<%= @cep_port %>
thrift.server.admin.username          =<%= @cep_username %>
thrift.server.admin.password          =<%= @cep_password %>
cep.stats.publisher.enabled           =true
lb.private.ip                         =
